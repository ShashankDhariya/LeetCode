class Solution {
public:
    int xorOperation(int n, int start) {
        int ans = start;
        vector<int> arr;
        
        for(int i=0;i<n;i++){
            arr.push_back(start);
            start += 2;
        }
        
        for(int i=1;i<n;i++){
            ans = ans^arr[i];
        }
        return ans;
    }
};
